				"          START_DATE as timestamp,",
				"          END_DATE as timestamp,",
				"          MERCH_TYPE as integer,",
				"          DEPT as integer,",
				"          CLASS as integer,",
				"          SUBCLASS as integer,",
				"          ITEM as string,",
				"          DIFF_ID as string,",
				"          LOCATION as integer,",
				"          DIVISION as integer,",
