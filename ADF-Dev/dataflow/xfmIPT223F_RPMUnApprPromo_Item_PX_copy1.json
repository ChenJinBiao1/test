				"     && split1@forest@CLASS == RPMPROMO@CLASS",
				"     && split1@forest@SUBCLASS == RPMPROMO@SUBCLASS",
				"     && split1@forest@ITEM == RPMPROMO@ITEM",
				"     && split1@forest@DIFF_ID == RPMPROMO@DIFF_ID",
