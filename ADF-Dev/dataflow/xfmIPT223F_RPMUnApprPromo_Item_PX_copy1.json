				"          START_DATE as timestamp,",
				"          END_DATE as timestamp,",
				"          MERCH_TYPE as integer,",
				"          DEPT as integer,",
				"          CLASS as integer,",
				"          SUBCLASS as integer,",
				"          ITEM as string,",
				"          DIFF_ID as string,",
				"          LOCATION as integer,",
				"          APPLY_TO_CODE as integer,",
				"          DIV as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RPMALLStore",
